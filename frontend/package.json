        "format": "prettier --write src/"
    },
    "dependencies": {
        "@heroicons/vue": "^2.1.1",
        "chart.js": "^4.4.1",
        "daisyui": "^4.6.1",
        "moment": "^2.30.1",
        "pinia": "^2.1.7",
        "vue": "^3.2.37",
        "vue-chartjs": "^5.3.0",
        "vue-router": "^4.2.5",
        "vue-toastification": "^2.0.0-rc.5"
    },
    "devDependencies": {
        "@babel/types": "^7.23.9",
        "@rushstack/eslint-patch": "^1.7.2",
        "@tsconfig/node18": "^18.2.2",
        "@types/node": "^20.11.13",
        "@vitejs/plugin-vue": "^4.6.2",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/tsconfig": "^0.4.0",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.56.0",
        "eslint-plugin-vue": "^9.21.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.2.4",
