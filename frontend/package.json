    "dependencies": {
        "@heroicons/vue": "^2.2.0",
        "chart.js": "^4.4.9",
        "daisyui": "^4.12.24",
        "moment": "^2.30.1",
        "pinia": "^2.3.1",
        "vue": "^3.5.13",
        "vue-chartjs": "^5.3.2",
        "vue-router": "^4.5.1",
        "vue-toastification": "2.0.0-rc.5"
    },
    "devDependencies": {
        "@babel/types": "^7.27.1",
        "@rushstack/eslint-patch": "^1.11.0",
        "@tsconfig/node18": "^18.2.4",
        "@types/node": "^20.17.46",
        "@vitejs/plugin-vue": "^4.6.2",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/tsconfig": "^0.4.0",
        "autoprefixer": "^10.4.21",
        "eslint": "^8.57.1",
        "eslint-plugin-vue": "^9.33.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.5.3",
